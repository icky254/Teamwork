{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "comma-dangle": 0,
        "no-undef": 0,
        "no-use-before-define": 0,
        "prefer-destructuring": 0,
        "camelcase": 0,
        "no-trailing-spaces": 0,
        "no-unused-vars": 0,
        ""
    }
}
